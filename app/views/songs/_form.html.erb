<%= form_for(song) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :artist_id %>
    <%= f.text_field :artist_id %>
    <%= f.label :genre_id %>
    <%= f.text_field :genre_id%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
